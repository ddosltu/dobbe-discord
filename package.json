{
	"name": "dobbe-discord",
	"version": "0.1.0",
	"description": "A Discord bot made by and for DDOS @ Datasektionen",
	"main": "index.js",
	"scripts": {
		"start": "npm run build && node build/index.js",
		"start:dotenv": "npm run build && node -r dotenv/config build/index.js",
		"dev": "nodemon",
		"build": "rimraf ./build && tsc",
		"test": "jest",
		"test:coverage": "jest --coverage",
		"lint": "eslint .",
		"format": "prettier --write \"**/*.+(js|ts|json|yml|yaml|css|md|vue)\""
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ddosltu/dobbe-discord.git"
	},
	"author": "DDOS",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/ddosltu/dobbe-discord/issues"
	},
	"homepage": "https://github.com/ddosltu/dobbe-discord#readme",
	"dependencies": {
		"@types/jest": "^26.0.15",
		"axios": "^0.21.0",
		"discord.js": "^12.4.1",
		"dotenv": "^8.2.0",
		"rimraf": "^3.0.2",
		"ts-node": "^9.0.0",
		"typescript": "^4.0.3"
	},
	"devDependencies": {
		"@types/node": "^14.14.3",
		"@typescript-eslint/eslint-plugin": "^4.6.0",
		"@typescript-eslint/parser": "^4.6.0",
		"eslint": "^7.12.0",
		"eslint-config-prettier": "^6.14.0",
		"eslint-plugin-prettier": "^3.1.4",
		"husky": "^4.3.0",
		"jest": "^26.6.1",
		"lint-staged": "^10.5.0",
		"nodemon": "^2.0.6",
		"prettier": "^2.1.2",
		"ts-jest": "^26.4.3"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.+(js|ts)": [
			"eslint --fix",
			"git add"
		],
		"*.+(json|css|md)": [
			"prettier --write",
			"git add"
		]
	}
}
